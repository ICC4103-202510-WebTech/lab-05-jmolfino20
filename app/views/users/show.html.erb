<h1>User <%= @user.id %> Details</h1>


<table>
    <tr>
        <th>Email</th>
        <td><%= @user.email %></td>
    </tr>
    <tr>
        <th>First Name</th>
        <td><%= @user.first_name %></td>
    </tr>
    <tr>
        <th>Last Name</th>
        <td><%= @user.last_name%></td>
    </tr>
    <tr>
        <th>Created At</th>
        <td><%= @user.created_at %></td>
    </tr>
    <tr>
        <th>Updated At</th>
        <td><%= @user.updated_at %></td>
    </tr>
</table>
<br>
<div>
    <%= link_to "Add new user", new_user_path %>
</div>
<br>

<div>
    <%= link_to "Back to Users", users_path%>
</div>